# mount point	fstype		device			[device2]

/boot           emmc        	/dev/bootimg           flags=display="Boot";backup=1
/logo           emmc        	/dev/logo              flags=display="Logo";backup=1
/cache          ext4        	/dev/block/mmcblk0p7   flags=display="Cache";backup=1
/data           ext4        	/dev/block/mmcblk0p8   flags=display="Data";backup=1
/sdcard		vfat		/dev/block/mmcblk1p1	/dev/block/mmcblk1	flags=display="Internal SD-card";storage;wipeingui;backup=1
/misc           emmc        	/dev/misc              flags=display="Misc"
/recovery       emmc        	/dev/recovery          flags=display="Recovery";backup=1
/system         ext4        	/dev/block/mmcblk0p6   flags=display="System";backup=1
/nvram          emmc        	/dev/nvram             flags=display="Nvram";backup=1
/uboot          emmc        	/dev/uboot             flags=display="Uboot";backup=1
/preload        ext4        	/dev/block/mmcblk0p5   flags=display="Preload";backup=1
/external_sd    vfat        	/dev/block/mmcblk1p1   flags=display="External SD-card";storage;wipeingui;removable
